* {
    // font-family: 'Open Sans' !important;
    font-family: 'Lato';
    outline: 0;
    font-size: 14px;
}

header,
nav,
article,
aside {
    padding: 0.25em;
    margin: 0.25em;
}

section {
    min-height: 100px;
}

html,
body,
.viewport {
    width: 100%;
    height: 100%;
    margin: 0;
}

.vbox {
    /* previous syntax */
    box-sizing: vertical;
    /* current syntax */
    display: flex;
    flex-direction: column;
}

.bgcolor {
    background-color: #e6e6e6;
}

.hbox {
    /* previous syntax */
    // display: box;
    box-sizing: horizontal;
    /* current syntax */
    display: flex;
    flex-direction: row;
    background-color: #e6e6e6;
}

.space-between {
    /* previous syntax */
    fallback: justify;
    /* current syntax */
    justify-content: space-between;
}

header,
    {
    height: 120px;
}

.main {
    /* previous syntax */
    fill-rule: 1;
    /* current syntax */
    flex: 1;
}

.className {
    background: #2196F3;
}

article {
    /* previous syntax */
    fill-rule: 5;
    /* current syntax */
    flex: 5;
}

aside,
nav {
    /* previous syntax */
    fill-rule: 1;
    /* current syntax */
    flex: 1;
}

.card {
    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
    transition: 0.3s;
    width: 100%;
    padding: 10px;
}

.card:hover {
    box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);
}

.container {
    padding: 2px 16px;
}

.searchbox {
    // justify-content: flex-end;
    align-items: center;
    width: 100%;
    min-width: 200px; // height: 100%;
    // min-height: 38px;
    border-radius: 10px;
    background-color: #EEEFF1;
}

::-webkit-scrollbar {
    width: 8px;
}

/* Track */

::-webkit-scrollbar-track {
    box-shadow: inset 0 0 5px grey;
    border-radius: 10px;
}

/* Handle */

::-webkit-scrollbar-thumb {
    background: grey;
    border-radius: 10px;
}

/* Handle on hover */

::-webkit-scrollbar-thumb:hover {
    background: #9e9b9b;
}

.cal-month-view .cal-day-cell {
