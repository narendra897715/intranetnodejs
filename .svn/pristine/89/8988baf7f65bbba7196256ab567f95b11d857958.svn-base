.text-area-style {
    .mat-form-field-wrapper {
        padding-bottom: 0px !important;
        padding-top: 5px !important;
    }
    .mat-form-field {
        width: 100%;
    }
}

.mat-form-field-prefix .mat-icon-button, .mat-form-field-suffix .mat-icon-button {
    color: #139c9a;
}


.mat-chip-list-wrapper {
    margin-bottom: 10px !important;
}

.background-color1 {
    background-image: grey;
}

.background-changed {
    background: linear-gradient(#ffbd6d, #ff917b);
}
.ui-calendar{
    width:100%;
}
.ui-calendar.ui-calendar-w-btn input{
    width:80%;
}
.angular-editor-textarea{
    min-height:100px !important;
    font-size:none;
}

.applyglobalclass {
  
    font-family: 'Lato';
    outline: 0;
    font-size: 14px;
    color: #444444;
}
.endtimeclassbeforeED {
    
    pointer-events: none;
}
.endtimeclassbeforeSD {
    
    pointer-events: auto;
}
.mat-form-field-infix{
    width:100% !important;
}
#editor{
    padding-top:1rem;
}
.angular-editor-button[title="Insert Image"],
.angular-editor-button[title="Unlink"],
.angular-editor-button[title="Insert Link"],
.angular-editor-button[title="Background Color"] {
  display: none;
}